#program to retrieve individual numbers from fibonnaci sequence through brute force??#

def fib():
  print("enter a positive integer")
  rep = int(input ())
  
  if rep > 0:
    n = 2
    one = 1
    two = 1
    three = 1
    
    while n < rep:
      three = one + two
      one = two
      two = three
      n = n + 1
      
    print (str(three) + " is number " + str(n) + " in the fibonacci sequence")
    
  else:
    fib()
